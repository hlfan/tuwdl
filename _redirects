# uses shorthands  a c d g q r s x
# shorthand to tuwEL
/q/*			/el/quiz/:splat
/a/*			/el/assign/:splat
/r/*			/el/resource/:splat
/c/*			/el/course/:splat

# parse path for tuwEL
/el/:path/:lva/:sem	/el/:path?idnumber=:lva-:sem
/el/:path/:id		/el/:path?id=:id

# specify correct subpath
/el/course/*		/tuwel/course/:splat
/el/*			/tuwel/mod/:splat

# redirect to TUWEL	https://tuwel.tuwien.ac.at/:splat/view.php
/tuwel/*		https://youtu.be/yR9K-G9lbDQ/:splat/view.php


# shorthand to TISS
/s/*			/tiss/courseRegistration/:splat
/x/*			/tiss/examDateList/:splat
/g/*			/tiss/groupList/:splat
/d/*			/tiss/educationDetails/:splat

# parse path for TISS
/tiss/:path/:lva/:sem	/tiss/:path?courseNr=:lva&semester=:sem

# redirect to TISS
/tiss/educationDetails	https://tiss.tuwien.ac.at/course/educationDetails.xhtml
/tiss/:path		https://tiss.tuwien.ac.at/education/course/:path.xhtml
